	<queryString language="SQL">
		<![CDATA[SELECT 
p.nombre as p_nombre,
p.duracion as p_duracion,
CASE WHEN p.fecha_inicio
	THEN DATE_FORMAT(p.fecha_inicio, "%d/%m/%Y") 
    ELSE '' 
END as p_fecha_inicio,
CASE WHEN p.fecha_fin
	THEN DATE_FORMAT(p.fecha_fin, "%d/%m/%Y")
    ELSE ''
END as p_fecha_fin,
CASE WHEN p.fecha_inicio_real
	THEN DATE_FORMAT(p.fecha_inicio_real, "%d/%m/%Y")
    ELSE ''
END as p_fecha_inicio_real,
CASE WHEN p.fecha_fin_real
	THEN DATE_FORMAT(p.fecha_fin_real, "%d/%m/%Y")
    ELSE ''
END as p_fecha_fin_real,
p.ejecucion_fisica_real as p_avance,
p.costo as p_planificado,

c.nombre as c_nombre,
c.duracion as c_duracion,
CASE WHEN c.fecha_inicio
	THEN DATE_FORMAT(c.fecha_inicio, "%d/%m/%Y") 
    ELSE '' 
END as c_fecha_inicio,
CASE WHEN c.fecha_fin
	THEN DATE_FORMAT(c.fecha_fin, "%d/%m/%Y")
    ELSE ''
END as c_fecha_fin,
CASE WHEN c.fecha_inicio_real
	THEN DATE_FORMAT(c.fecha_inicio_real, "%d/%m/%Y")
    ELSE ''
END as c_fecha_inicio_real,
CASE WHEN c.fecha_fin_real
	THEN DATE_FORMAT(c.fecha_fin_real, "%d/%m/%Y")
    ELSE ''
END as c_fecha_fin_real,
c.costo as c_planificado,

sc.nombre as sc_nombre,
sc.duracion as sc_duracion,
CASE WHEN sc.fecha_inicio
	THEN DATE_FORMAT(sc.fecha_inicio, "%d/%m/%Y") 
    ELSE '' 
END as sc_fecha_inicio,
CASE WHEN sc.fecha_fin
	THEN DATE_FORMAT(sc.fecha_fin, "%d/%m/%Y")
    ELSE ''
END as sc_fecha_fin,
CASE WHEN sc.fecha_inicio_real
	THEN DATE_FORMAT(sc.fecha_inicio_real, "%d/%m/%Y")
    ELSE ''
END as sc_fecha_inicio_real,
CASE WHEN sc.fecha_fin_real
	THEN DATE_FORMAT(sc.fecha_fin_real, "%d/%m/%Y")
    ELSE ''
END as sc_fecha_fin_real,
sc.costo as sc_planificado,

pr.nombre as pr_nombre,
pr.duracion as pr_duracion,
CASE WHEN pr.fecha_inicio
	THEN DATE_FORMAT(pr.fecha_inicio, "%d/%m/%Y") 
    ELSE '' 
END as pr_fecha_inicio,
CASE WHEN pr.fecha_fin
	THEN DATE_FORMAT(pr.fecha_fin, "%d/%m/%Y")
    ELSE ''
END as pr_fecha_fin,
CASE WHEN pr.fecha_inicio_real
	THEN DATE_FORMAT(pr.fecha_inicio_real, "%d/%m/%Y")
    ELSE ''
END as pr_fecha_inicio_real,
CASE WHEN pr.fecha_fin_real
	THEN DATE_FORMAT(pr.fecha_fin_real, "%d/%m/%Y")
    ELSE ''
END as pr_fecha_fin_real,
pr.costo as pr_planificado,

sp.nombre as sp_nombre,
sp.duracion as sp_duracion,
CASE WHEN sp.fecha_inicio
	THEN DATE_FORMAT(sp.fecha_inicio, "%d/%m/%Y") 
    ELSE '' 
END as sp_fecha_inicio,
CASE WHEN sp.fecha_fin
	THEN DATE_FORMAT(sp.fecha_fin, "%d/%m/%Y")
    ELSE ''
END as sp_fecha_fin,
CASE WHEN sp.fecha_inicio_real
	THEN DATE_FORMAT(sp.fecha_inicio_real, "%d/%m/%Y")
    ELSE ''
END as sp_fecha_inicio_real,
CASE WHEN sp.fecha_fin_real
	THEN DATE_FORMAT(sp.fecha_fin_real, "%d/%m/%Y")
    ELSE ''
END as sp_fecha_fin_real,
sp.costo as sp_planificado,
ifnull((
select sum(ifnull(mv.ejecutado,0)) 
from sipro.proyecto p2, sipro.prestamo pres2, sipro.componente c2, sipro.subcomponente sc2, sipro.producto pr2, sipro.subproducto sp2, sipro_analytic.mv_ep_ejec_asig_vige mv
where c2.proyectoid=p2.id and sc2.componenteid=c2.id and pr2.subcomponenteid=sc2.id and sp2.productoid=pr2.id
and mv.ejercicio=year(now())
and p2.id= $P{proyectoId} 
and p2.prestamoid=pres2.id
and mv.fuente=CAST(substring(pres2.codigo_presupuestario,1,2) as integer)
and mv.organismo=CAST(substring(pres2.codigo_presupuestario, 3,4) as integer)
and mv.correlativo=CAST(substring(pres2.codigo_presupuestario, 7,4) as integer)
and mv.entidad=sp2.entidad
and mv.programa=sp2.programa
and mv.subprograma=sp2.subprograma 
and mv.proyecto=sp2.proyecto
and mv.actividad=sp2.actividad
and mv.obra=sp2.obra
and mv.renglon=sp2.renglon
and mv.geografico=sp2.ubicacion_geografica
and mv.mes<=month(now())
and sp2.id=sp.id
),0) sp_devengado,

ifnull((select sum(mv.asignado) as asignado
from sipro.proyecto p2, sipro.prestamo pres2, sipro.componente c2, sipro.subcomponente sc2, sipro.producto pr2, sipro.subproducto sp2, sipro_analytic.mv_ep_ejec_asig_vige mv
where c2.proyectoid=p2.id and sc2.componenteid=c2.id and pr2.subcomponenteid=sc2.id and sp2.productoid=pr2.id
and mv.ejercicio=year(now())
and p2.id= $P{proyectoId} 
and p2.prestamoid=pres.id
and mv.fuente=CAST(substring(pres2.codigo_presupuestario,1,2) as integer)
and mv.organismo=CAST(substring(pres2.codigo_presupuestario, 3,4) as integer)
and mv.correlativo=CAST(substring(pres2.codigo_presupuestario, 7,4) as integer)
and mv.entidad=sp2.entidad
and mv.programa=sp2.programa
and mv.subprograma=sp2.subprograma 
and mv.proyecto=sp2.proyecto
and mv.actividad=sp2.actividad
and mv.obra=sp2.obra
and mv.renglon=sp2.renglon
and mv.geografico=sp2.ubicacion_geografica
and mv.mes=0
and sp2.id=sp.id
group by mv.mes
),0) sp_asignado,

ifnull((select sum(mv.modificaciones) 
from sipro.proyecto p2, sipro.prestamo pres2, sipro.componente c2, sipro.subcomponente sc2, sipro.producto pr2, sipro.subproducto sp2, sipro_analytic.mv_ep_ejec_asig_vige mv
where c2.proyectoid=p2.id and sc2.componenteid=c2.id and pr2.subcomponenteid=sc2.id and sp2.productoid=pr2.id
and mv.ejercicio=year(now())
and p2.id= $P{proyectoId} 
and p2.prestamoid=pres.id
and mv.fuente=CAST(substring(pres2.codigo_presupuestario,1,2) as integer)
and mv.organismo=CAST(substring(pres2.codigo_presupuestario, 3,4) as integer)
and mv.correlativo=CAST(substring(pres2.codigo_presupuestario, 7,4) as integer)
and mv.entidad=sp2.entidad
and mv.programa=sp2.programa
and mv.subprograma=sp2.subprograma 
and mv.proyecto=sp2.proyecto
and mv.actividad=sp2.actividad
and mv.obra=sp2.obra
and mv.renglon=sp2.renglon
and mv.geografico=sp2.ubicacion_geografica
and mv.mes<month(now())
and sp2.id=sp.id),0) sp_vigente
FROM sipro.proyecto p, sipro.componente c, sipro.subcomponente sc, sipro.producto pr, sipro.subproducto sp, sipro.prestamo pres
WHERE c.proyectoid=p.id and sc.componenteid=c.id and pr.subcomponenteid=sc.id and sp.productoid=pr.id and p.prestamoid=pres.id
and p.id=$P{proyectoId} ;]]>
